    "test": "nyc --reporter=text-summary --reporter=html env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'tests/**/*.ts'",
    "test:spec": "mocha -r ts-node/register modules/**/*-spec.ts",
